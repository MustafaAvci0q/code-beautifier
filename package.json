{
    "name": "code-beautifier",
    "displayName": "Beautify css/sass/scss/less",
    "description": "Beautify css, sass and less code (extension for Visual Studio Code)",
    "version": "2.0.0",
    "publisher": "michelemelluso",
    "license": "Apache-2.0",
    "icon": "icon.png",
    "bugs": {
        "url": "https://github.com/tierratelematics/code-beautifier/issues",
        "email": "michele.jr.melluso@gmail.com"
    },
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
        "Languages"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "configuration": {
            "type": "object",
            "title": "Beautify css/sass/scss/less",
            "properties": {
                "beautify.tabSize": {
                    "type": "number",
                    "default": 4,
                    "description": "Tab Size"
                },
                "beautify.options": {
                    "type": "object",
                    "default": {},
                    "description": "Tab Size"
                }
            }
        },
        "commands": [
            {
                "command": "beautify.format",
                "title": "Beautify css/sass/scss/less"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "node ./node_modules/typescript/bin/tsc -p .",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "dependencies": {
        "js-beautify": "^1.7.5"
    },
    "devDependencies": {
        "typescript": "^2.8.1",
        "vscode": "^1.1.14",
        "mocha": "^5.0.5",
        "@types/node": "^9.6.1",
        "@types/mocha": "^5.0.0",
        "@types/js-beautify": "^0.0.31"
    }
}
