{
    "name": "code-beautifier",
    "displayName": "Beautify css/sass/scss/less",
    "description": "Beautify css, sass and less code (extension for Visual Studio Code)",
    "version": "1.5.2",
    "publisher": "michelemelluso",
    "license": "Apache-2.0",
    "icon": "icon.png",
    "bugs": {
        "url": "https://github.com/tierratelematics/code-beautifier/issues",
        "email": "michele.jr.melluso@gmail.com"
    },
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
        "Languages"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [
            {
                "command": "beautify.format",
                "title": "Beautify this code"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "dependencies": {
        "js-beautify": "^1.6.4"
    },
    "devDependencies": {
        "typescript": "^2.0.7",
        "vscode": "^1.0.3",
        "mocha": "^3.1.2",
        "@types/node": "^6.0.46",
        "@types/mocha": "^2.2.32"
    }
}